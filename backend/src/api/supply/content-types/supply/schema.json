{
  "kind": "collectionType",
  "collectionName": "supplies",
  "info": {
    "singularName": "supply",
    "pluralName": "supplies",
    "displayName": "Supply",
    "description": "Ingredient supply orders from suppliers"
  },
  "options": {
    "draftAndPublish": false
  },
  "attributes": {
    "supplierName": {
      "type": "string",
      "required": true,
      "maxLength": 200
    },
    "status": {
      "type": "enumeration",
      "enum": ["draft", "ordered", "shipped", "received", "cancelled"],
      "default": "draft",
      "required": true
    },
    "items": {
      "type": "json"
    },
    "totalCost": {
      "type": "decimal",
      "default": 0,
      "min": 0
    },
    "notes": {
      "type": "text",
      "maxLength": 1000
    },
    "orderedAt": {
      "type": "datetime"
    },
    "shippedAt": {
      "type": "datetime"
    },
    "receivedAt": {
      "type": "datetime"
    },
    "createdBy_barista": {
      "type": "string",
      "maxLength": 200
    },
    "receivedBy": {
      "type": "string",
      "maxLength": 200
    },
    "shift": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::shift.shift"
    }
  }
}
